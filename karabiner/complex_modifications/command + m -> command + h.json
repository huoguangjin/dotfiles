{
  "title": "command + m -> command + h",
  "maintainers": [
    "hgj"
  ],
  "rules": [
    {
      "description": "command + m -> command + h",
      "manipulators": [
        {
          "from": {
            "key_code": "m",
            "modifiers": {
              "mandatory": [
                "left_command"
              ]
            }
          },
          "to": [
            {
              "key_code": "h",
              "modifiers": [
                "left_command"
              ]
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "m",
            "modifiers": {
              "mandatory": [
                "right_command"
              ]
            }
          },
          "to": [
            {
              "key_code": "h",
              "modifiers": [
                "right_command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
